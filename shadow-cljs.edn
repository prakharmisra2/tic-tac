;; shadow-cljs configuration
{:source-paths
 ["src/dev"
  "src/main"
  "src/test"]

 :dependencies
 [[reagent "1.2.0"]
  [cljs-ajax "0.8.4"]
  [binaryage/devtools "1.0.7"]]

 :dev-http
